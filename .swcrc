{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
    "module": {
      "type": "commonjs"
    },
  "jsc": {
      "target": "ESNext",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true,
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "baseUrl": "./",
    "paths": {
        "@src/*": ["./src/*"],
        "@shared/*": ["./src/Shared/*"],
        "@app/*": ["./src/App/*"],
        "@auth/*": ["./src/Auth/*"],
        "@test/*": ["test/*"]

    },
  },
  "minify": false,
}
